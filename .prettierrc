{
    "trailingComma": "all",
    "arrowParens": "avoid",
    "singleQuote": true,
    "printWidth": 80,
    "useTabs": false,
    "tabWidth": 2,
    "semi": true,
    "importOrder": [
        "^(react|react-native)$",
        "<THIRD_PARTY_MODULES>",
        "^@",
        "~types",
        "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
}